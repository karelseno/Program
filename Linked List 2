#include <iostream>
#include <stdlib.h>

using namespace std;
struct Node {
    int data;
    struct Node*next;
};
struct Node*head=NULL;
void printList () {
    struct Node*point=head;
    while (point!= NULL){
        cout<<" "<<point->data<< " ";
        point=point->next;
     }
}
    void insertAtbegin (int data) {
        struct Node*lk=(struct Node*)malloc(sizeof(struct Node));
        lk-> data=data;
        lk-> next=head;
        head=lk;
    }
    int main (){
        insertAtbegin (12);
        insertAtbegin (22);
        insertAtbegin (30);
        insertAtbegin (44);
        insertAtbegin (50);
        printList();
        
        return 0;
  }
  
